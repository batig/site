.navbar-fixed-top {
  margin-bottom: 0;
  
  .navbar-inner {
    padding: 0;
    background-image: none;
    background-color:rgb(255,255,255);
    background-color:rgba(255,255,255,0.4);
    @include box-shadow(0px,5px,10px,0px,rgba(0,0,0,0.1));
  }
}

.navbar-logo {
  height: 20px;
  width: 120px;
  text-indent: -9999px;
  background-image: image-url("sprite.png");
  background-position: -305px -70px;
}

.navbar-fixed-top {
  margin-bottom: 0;
  
  .navbar-inner {
    padding: 10px;
    background-image: none;
    background-color: rgb(255,255,255);
    box-shadow: none;
  }
}

.navbar-logo {
  height: 20px;
  width: 120px;
  text-indent: -9999px;
  background-image: image-url("sprite.png");
  background-position: -305px -70px;
}

.navbar a {
  border-bottom: none;
  text-transform: uppercase;
}

.navbar .nav > li > a {
  text-shadow: none;
  color: #616161;
  font: 400 14px $ubuntu;
}
